{
	"name": "Badass Biz Photos",
	"version": "0.9",
	"manifest_version": 2,
	"description": "Yelp Hackathon 9.0 project",
	"icons": {
		"16":  "icon.png",
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://*.yelp.com/biz*",
				"http://*.yelp.com/user_photos*"
			],
			"css": [
				"badass-biz-photos.css",
				"galleria/themes/hackathon9/galleria.hackathon9.css"
			],
			"js": [
				"jquery-1.8.2.js",
				"galleria/galleria-1.2.8.js",
				"galleria/themes/hackathon9/galleria.hackathon9.js",
				"badass-biz-photos.js"
			]
		}
	],
	"web_accessible_resources": [
		"galleria/themes/hackathon9/classic-map.png",
		"galleria/themes/hackathon9/classic-loader.gif"
	]
}
